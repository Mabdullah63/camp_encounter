<h3>
  <center>
    <div class="mt-3">
      Hello <%= current_user.first_name %> (<%= current_user.email %>) Welocome to Camp Encounter
    </div>
    <div>
      List Of Registered Users Given Below
    </div>
  </center>
</h3>
<table class="table table-striped table-bordered table-hover mt-3">
  <thead class="table-dark">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>phone No</th>
      <th>Country Code</th>
      <th>Country</th>
      <th colspan="3"> </th>
    </tr>
  </thead>
<tbody>
  <%= render @users %>
</tbody>
</table>
<div class="mb-5">
  <center class="mb-3">
    <%= pagy_bootstrap_nav(@pagy).html_safe %>
  </center>
</div>

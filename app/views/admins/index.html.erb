<h3>
  <center>
    Hello <%= current_user.first_name %> (<%= current_user.email %>) Welocome to Camp Encounter
    <div></div>
    List Of Registered Users Given Below
  </center>
</h3><div></div>
<div class="col-md-4">
  <%= form_with(url: admins_path, method: :get) do |f| %>
    <%= f.text_field :query, class: 'search-bar', placeholder: 'Search by Name, Email, country, or phone' %>
    <%= f.submit "Search", class: 'btn btn-secondary' %>
  <% end %>
</div><div></div>
</h3><div></div>
<table class="table table-striped table-bordered table-hover">
  <thead class="table-dark">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>phone No</th>
      <th>Country Code</th>
      <th>Country</th>
      <th colspan="3"> </th>
    </tr>
  </thead>
  <tbody>
    <%= render 'users', users: @users %>
  </tbody>
</table>
<center>
  <%= pagy_bootstrap_nav(@pagy).html_safe %>
</center>
<div class="mb-3"></div>
<div class="mb-3"></div>
